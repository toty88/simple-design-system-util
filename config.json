{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "ts": {
      "transformGroup": "js",
      "files": [
        {
          "format": "javascript/es6",
          "destination": "src/style-dictionary/colors.ts"
        },
        {
          "format": "typescript/es6-declarations",
          "destination": "src/style-dictionary/colors.d.ts"
        }
      ]
    }
  }
}
